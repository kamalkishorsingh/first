{
  "name": "evenup-curator",
  "version": "2.9.9",
  "author": "Justin Lambert <jlambert@letsevenup.com>",
  "summary": "Installs elasticsearch curator and schedules cron jobs",
  "license": "Apache-2.0",
  "source": "https://github.com/evenup/evenup-curator",
  "project_page": "https://github.com/evenup/evenup-curator",
  "issues_url": "https://github.com/evenup/evenup-curator/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">=4.2.0 <5.0.0"}
  ],
  "tags": [
    "curator",
    "elasticsearch"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ]
}
